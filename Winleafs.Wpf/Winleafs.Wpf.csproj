		<ProjectReference Include="..\Winleafs.Orchestration\Winleafs.Orchestration.csproj" />
		<ProjectReference Include="..\Winleafs.Server\Winleafs.Server.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Resource Include="Icons\icon.ico">
